{% extends 'Blog/base.html.twig' %}

{% block title %}
    {{ parent() }} - My Articles
{% endblock %}

{% block leftContent %}
    {% for article in articles %}
        {% if article.enabled %}
            <article>
                <header>
                    <h1>{{ article.name }}</h1>
                </header>
                <p>{{ article.content }}</p>
                <footer>
                    <time datetime="{{ article.date|date('c') }}">{{ article.date|date('d-m-Y') }}</time>
                </footer>
            </article>
        {% endif %}
    {% endfor %}
{% endblock %}

